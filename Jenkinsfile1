pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/110-Shivanand/jenk-ins.git'
            }
        }

        stage('Run Python Script') {
            steps {
                bat 'C:\\Users\\K_Shiva\\AppData\\Local\\Programs\\Python\\Python311\\python.exe linked_list.py'
            }
        }
    }

    post {
        success {
            echo 'Build completed successfully.'
        }
        failure {
            echo 'Build failed.'
        }
    }
}
